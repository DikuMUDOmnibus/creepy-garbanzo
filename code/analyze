splint -DLINUX -warnposix  -incondefs -nullstate -unrecog -nullpass -aliasunique -mustfreeonly -mustfreefresh -mayaliasunique -compdef -retvalbool -temptrans -boolops -retvalint -branchstate -statictrans -nullret -nullassign -bufferoverflowhigh -retvalother -paramuse +charindex -predboolint -shadow -nestedextern -globstate -nullderef -compmempass -evalorder -usereleased -compdestroy -dependenttrans -formatconst -exitarg -unqualifiedtrans -onlytrans -ifempty -castfcnptr -realcompare -casebreak -immediatetrans -initallelements -observertrans -kepttrans -noret -duplicatequals -modobserver -exportlocal -usedef nanny.c > splint.out
